{
  "name": "my-private-label",
  "version": "0.0.1",
  "description": "front end project for the private label project",
  "private": true,
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run typings-install",
    "typings-install": "typings install",
    "clean": "rimraf build",
    "build:dev": "npm run clean && webpack --config ./config/webpack.dev.js --progress --profile --bail",
    "build:test": "npm run clean && webpack --config ./config/webpack.test.js --progress --profile --bail",
    "build:prod": "cross-env NODE_ENV=production npm run clean && webpack --config ./config/webpack.prod.js --progress --profile --bail -p",
    "test": "karma start ./config/karma.config.js",
    "serve": "webpack-dev-server --config config/webpack.dev.js --progress --profile --colors --watch --display-error-details --display-cached",
    "perf": "cross-env NODE_ENV=production npm run clean && webpack --config ./config/webpack.prod.js --progress --profile --bail -p -c --json > stats.json"
  },
  "repository": {
    "type": "svn",
    "url": "https://svn6.sentry.com/svn/CP_Portal_Agent/customer/my-private-label/trunk/"
  },
